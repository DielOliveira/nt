<p id="notice"><%= notice %></p>

<h1>Listing Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Senha</th>
      <th>Cadastro</th>
      <th>Datainclusao</th>
      <th>Dataultimologin</th>
      <th>Flagativo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= usuario.email %></td>
        <td><%= usuario.senha %></td>
        <td><%= usuario.cadastro %></td>
        <td><%= usuario.datainclusao %></td>
        <td><%= usuario.dataultimologin %></td>
        <td><%= usuario.flagativo %></td>
        <td><%= link_to 'Show', usuario %></td>
        <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
        <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario', new_usuario_path %>
